{
  "name": "bottender",
  "version": "0.14.0",
  "license": "MIT",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "keywords": [
    "messaging",
    "messenger",
    "line",
    "slack",
    "telegram",
    "bot",
    "chatbot",
    "framework"
  ],
  "homepage": "https://bottender.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoctol/bottender.git"
  },
  "files": [
    "bin",
    "lib",
    "express.js",
    "koa.js",
    "micro.js",
    "restify.js"
  ],
  "bin": {
    "bottender": "bin/cli.js",
    "btd": "bin/cli.js"
  },
  "scripts": {
    "build:docs": "node scripts/build-docs",
    "build": "npm run clean && babel src -d lib --ignore __tests__",
    "clean": "rimraf lib",
    "check": "flow check",
    "lint": "eslint bin src scripts examples",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "precommit": "lint-staged",
    "test": "npm run lint:fix && npm run check && npm run testonly",
    "testonly": "jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit",
    "testonly:watch": "jest --watch",
    "preversion": "npm test",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-yoctol-base": "^0.14.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "flow-bin": "^0.57.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-junit": "^3.1.0",
    "lint-staged": "^4.3.0",
    "ngrok": "^2.2.23",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "body-parser": "^1.18.2",
    "camel-case": "^3.0.0",
    "chalk": "^2.3.0",
    "child_process": "^1.0.2",
    "cli-table2": "^0.2.0",
    "cross-spawn": "^5.1.0",
    "date-fns": "^1.29.0",
    "debug": "^3.1.0",
    "delay": "^2.0.0",
    "express": "^4.16.2",
    "figures": "^2.0.0",
    "fs-extra": "^4.0.2",
    "import-fresh": "^2.0.0",
    "inquirer": "^3.3.0",
    "invariant": "^2.2.2",
    "ioredis": "^3.2.1",
    "jfs": "^0.2.6",
    "joi": "11.3.4",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "messaging-api-line": "^0.5.2",
    "messaging-api-messenger": "^0.5.3",
    "messaging-api-slack": "^0.5.6",
    "messaging-api-telegram": "^0.5.2",
    "micro": "^9.0.0",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.33",
    "pascal-case": "^2.0.1",
    "prompt-confirm": "^1.2.0",
    "readline": "^1.3.0",
    "restify": "^6.2.3",
    "shortid": "^2.2.8",
    "stringify-object": "^3.2.1",
    "thenify": "^3.3.0",
    "validate-npm-package-name": "^3.0.0",
    "warning": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFiles": [
      "./test/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "timers": "fake",
    "resetModules": true,
    "resetMocks": true
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
